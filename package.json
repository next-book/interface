{
  "name": "nb-base",
  "version": "0.1.13",
  "description": "Next-bookâ€™s base browser library",
  "repository": "https://github.com/next-book/nb-base",
  "main": "lib/index.js",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "rm -r dist && mkdir dist && npm run build:js && npm run build:css && npm run bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src/js --out-dir lib --extensions \".ts,.tsx\"",
    "build:css": "node-sass src/scss/style.scss dist/nb-base.css",
    "bundle": "webpack --mode production",
    "test": "jest"
  },
