    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cuid": "^2.1.6",
    "fscreen": "^1.0.2",
    "keycode": "^2.2.0",
